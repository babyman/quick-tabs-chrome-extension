{
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "assets/icon-32.png",
    "default_title": "Quick Tabs",
    "default_popup": "popup.html"
  },
  "description": "A keyboard centric most recently used (MRU) ordered tab list plugin with search and switch functionality inspired by IntelliJ IDEA",
  "icons": {
    "128": "assets/icon-128.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png"
  },
  "minimum_chrome_version": "25",
  "manifest_version": 2,
  "name": "Quick Tabs",
  "options_page": "options.html",
  "permissions": [ "tabs", "bookmarks", "history", "http://*/*", "https://*/*" ],
  "update_url": "http://clients2.google.com/service/update2/crx",

  "version": "2017.5.10",

  "commands": {
    "_execute_browser_action": {
       "suggested_key": {
         "default": "Ctrl+E",
         "mac": "Command+E",
         "linux": "Ctrl+Q"
       }
    },
    "quick-prev-tab": {
      "description": "Quick next tab (suggested Ctrl+J)"
    },
    "quick-next-tab": {
      "description": "Quick previous tab (suggested Ctrl+K)"
    }
  }
}
